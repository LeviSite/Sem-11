    "\n",
    "df = pd.read_csv(\"breast_cancer_key_genes.tsv\",  sep=\"\\t\", index_col=0)\n",
    "\n",
    "sns.kdeplot(x=df[\"ESR1\"], y=df[\"PGR\"])\n",
    "plt.tight_layout()\n",
    "plt.savefig(\"11.5.jpeg\")\n",
    "plt.close()"
